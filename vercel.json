{
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/route1/(.*)",
        "dest": "/src/routes/route1/index.js"
      },
      {
        "src": "/route2/(.*)",
        "dest": "/src/routes/route2/index.js"
      },
      {
        "src": "/route3/(.*)",
        "dest": "/src/routes/route3/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }